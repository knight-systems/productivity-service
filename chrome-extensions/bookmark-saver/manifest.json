{
  "manifest_version": 3,
  "name": "Obsidian Bookmark Saver",
  "version": "1.1.0",
  "description": "Save bookmarks to Obsidian with AI-generated tags",
  "permissions": ["activeTab", "notifications", "scripting"],
  "host_permissions": [
    "https://el5c54bhs2.execute-api.us-east-1.amazonaws.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Save to Obsidian"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
